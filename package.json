{
  "name": "babylon",
  "type": "module",
  "version": "0.0.0",
  "private": true,
  "packageManager": "pnpm@10.16.1",
  "engines": {
    "node": ">=22.13.1",
    "pnpm": ">=10.15.1"
  },
  "scripts": {
    "start": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "import-meta-env": "import-meta-env -x .env.example -p dist/index.html",
    "lint": "eslint",
    "lint:fix": "eslint --fix",
    "test:unit": "vitest run",
    "test:coverage": "vitest run --coverage",
    "prepare": "simple-git-hooks",
    "preinstall": "npx only-allow pnpm"
  },
  "dependencies": {
    "@ant-design/icons": "6.0.0",
    "@babylonjs/core": "^8.24.0",
    "@babylonjs/inspector": "^8.24.0",
    "@babylonjs/loaders": "^8.24.0",
    "@babylonjs/materials": "^8.24.0",
    "@eyevinn/webrtc-player": "0.13.0",
    "@rcuse/core": "0.25.0",
    "@tanstack/react-query": "5.84.1",
    "@tanstack/react-router": "1.130.12",
    "@tanstack/react-virtual": "3.13.12",
    "@yunzhen/request": "1.0.0",
    "ahooks": "3.9.5",
    "antd": "5.26.7",
    "autofit.js": "3.2.8",
    "clsx": "2.1.1",
    "dayjs": "1.11.13",
    "es-toolkit": "1.39.10",
    "gsap": "3.13.0",
    "howler": "2.2.4",
    "mitt": "^3.0.1",
    "motion": "12.23.12",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "react-infinite-scroll-hook": "5.0.2",
    "stats.js": "^0.17.0",
    "type-fest": "4.41.0",
    "zod": "4.0.15",
    "zustand": "5.0.7"
  },
  "devDependencies": {
    "@antfu/eslint-config": "5.2.0",
    "@biomejs/biome": "2.1.3",
    "@commitlint/cli": "19.8.1",
    "@commitlint/config-conventional": "19.8.1",
    "@commitlint/types": "19.8.1",
    "@eslint-react/eslint-plugin": "1.52.3",
    "@import-meta-env/cli": "0.7.3",
    "@tanstack/eslint-plugin-query": "5.83.1",
    "@tanstack/react-router-devtools": "1.130.13",
    "@tanstack/router-plugin": "1.130.15",
    "@testing-library/jest-dom": "6.6.3",
    "@types/howler": "2.2.12",
    "@types/node": "24.0.12",
    "@types/qs": "6.14.0",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "@types/stats.js": "^0.17.4",
    "@vitejs/plugin-react-swc": "3.10.2",
    "eslint": "9.32.0",
    "eslint-plugin-react-hooks": "5.2.0",
    "eslint-plugin-react-refresh": "0.4.20",
    "jsdom": "26.1.0",
    "less": "4.4.0",
    "lint-staged": "16.1.4",
    "simple-git-hooks": "2.13.1",
    "typescript": "~5.8.3",
    "vite": "7.1.0",
    "vite-plugin-html": "3.2.2",
    "vite-plugin-svgr": "4.3.0",
    "vitest": "3.2.4"
  },
  "simple-git-hooks": {
    "commit-msg": "npx --no -- commitlint --edit $1",
    "pre-commit": "npx lint-staged",
    "preserveUnused": [
      "commit-msg"
    ]
  },
  "lint-staged": {
    "*": "eslint --fix"
  }
}
